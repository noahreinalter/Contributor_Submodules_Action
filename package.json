{
  "name": "contributor_submodules_action",
  "version": "0.0.0",
  "private": true,
  "description": "A Github Action that automates adding submodulesTypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "fix-lint": "eslint src/**/*.ts --fix",
    "package": "ncc build -m --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noahreinalter/Contributor_Submodules_Action.git"
  },
  "keywords": [
    "actions",
    "node"
  ],
  "author": "noahreinalter",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.0",
    "@actions/exec": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.6.4",
    "@typescript-eslint/parser": "^5.32.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.21.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^26.8.1",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.7.4"
  }
}
