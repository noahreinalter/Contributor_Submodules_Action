{
  "name": "contributor_submodules_action",
  "version": "2.1.0",
  "private": true,
  "type": "module",
  "description": "A Github Action that automates adding submodules",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"**/*.ts\"",
    "format-check": "prettier --check \"**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "fix-lint": "eslint src/**/*.ts --fix",
    "package": "ncc build -m --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noahreinalter/Contributor_Submodules_Action.git"
  },
  "keywords": [
    "actions",
    "node"
  ],
  "author": "noahreinalter",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^3.0.0",
    "@actions/exec": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@types/node": "^25.2.2",
    "@typescript-eslint/parser": "^8.54.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.39.2",
    "eslint-plugin-github": "^6.0.0",
    "globals": "^17.3.0",
    "js-yaml": "^4.1.1",
    "prettier": "3.8.1",
    "typescript": "^5.9.3"
  }
}