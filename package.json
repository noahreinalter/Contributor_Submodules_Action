{
  "name": "contributor_submodules_action",
  "version": "2.0.4",
  "private": true,
  "description": "A Github Action that automates adding submodules",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"**/*.ts\"",
    "format-check": "prettier --check \"**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "fix-lint": "eslint src/**/*.ts --fix",
    "package": "ncc build -m --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noahreinalter/Contributor_Submodules_Action.git"
  },
  "keywords": [
    "actions",
    "node"
  ],
  "author": "noahreinalter",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "@typescript-eslint/parser": "^8.23.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.19.0",
    "eslint-plugin-github": "^5.1.8",
    "js-yaml": "^4.1.0",
    "prettier": "3.5.0",
    "typescript": "^5.7.3"
  }
}