{
  "name": "contributor_submodules_action",
  "version": "2.0.2",
  "private": true,
  "description": "A Github Action that automates adding submodules",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "fix-lint": "eslint src/**/*.ts --fix",
    "package": "ncc build -m --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noahreinalter/Contributor_Submodules_Action.git"
  },
  "keywords": [
    "actions",
    "node"
  ],
  "author": "noahreinalter",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "@typescript-eslint/parser": "^6.2.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.46.0",
    "eslint-plugin-github": "^4.9.2",
    "js-yaml": "^4.1.0",
    "prettier": "3.0.0",
    "typescript": "^5.1.6"
  }
}
